{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "bothub-proxy-dev",
        "image": "vctrferreira/bothub-nlp-dev",
        "essential": true,
        "memoryReservation": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "environment": [
          {
            "name": "APP_REPOSITORY_URL",
            "value": "https://github.com/push-flow/bothub-nlp.git"
          },
          {
            "name": "APP_REPOSITORY_BRANCH",
            "value": "develop"
          },
          {
            "name": "BOTHUB_POSTGRES_USER",
            "value": "postgres"
          },
          {
            "name": "BOTHUB_POSTGRES_PASSWORD",
            "value": "postgres"
          },
          {
            "name": "BOTHUB_POSTGRES_DB",
            "value": "bothub"
          },
          {
            "name": "BOTHUB_POSTGRES_PORT",
            "value": "5432"
          },
          {
            "name": "BOTHUB_POSTGRES",
            "value": "0.0.0.0"
          },
          {
            "name": "BOTHUB_REDIS",
            "value": "0.0.0.0"
          },
          {
            "name": "BOTHUB_REDIS_PORT",
            "value": "6379"
          },
          {
            "name": "BOTHUB_REDIS_DB",
            "value": "2"
          }
        ]
      }
    ]
  }